{
    "generate": {
        "options": {
            "basePath": "public/",
            "cache": ["../"],
            "verbose": true,
            "timestamp": true,
            "hash": true,
            "preferOnline": true
        },
        "src": [
            "components/requirejs/require.js",
            "build/javascripts/*.js",
            "build/javascripts/*.map",
            "stylesheets/*.css"
        ],
        "dest": "public/manifest.appcache"
    }
}